<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Extranet\ConcoursBundle\Entity\Concours" table="concours">
    <id name="idconcours" type="integer" column="idConcours">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titre" type="string" column="titre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datelimite" type="date" column="dateLimite" nullable="false"/>
    <field name="datecreation" type="date" column="dateCreation" nullable="true"/>
  </entity>
</doctrine-mapping>
