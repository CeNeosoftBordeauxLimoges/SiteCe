<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Extranet\ConcoursBundle\Entity\Reponse" table="reponse">
    <indexes>
      <index name="FK_REPONSE_CONCOURS" columns="idConcours"/>
    </indexes>
    <id name="idreponse" type="integer" column="idReponse">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelle" type="string" column="libelle" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="reponsecorrecte" type="boolean" column="reponseCorrecte" nullable="false"/>
    <many-to-one field="idconcours" target-entity="Concours" fetch="LAZY">
      <join-columns>
        <join-column name="idConcours" referenced-column-name="idConcours"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
